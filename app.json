{
  "expo": {
    "name": "FotMob",
    "slug": "Score-Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icons/splash-icon.png",
    "scheme": "ForMob",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icons/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ilafer.ScoreTracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app_icons/splash-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_icons/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fef489b-ed3c-472d-b8e0-61e41ae430bc"
      }
    }
  }
}
